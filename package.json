{
  "name": "trace-extractor",
  "version": "1.0.0",
  "description": "Trace Extractor - Extract and convert Cursor conversations to markdown with VSCode extension and CLI interface",
  "main": "./extension.js",
  "scripts": {
    "start": "node cli.js",
    "cli": "node cli.js",
    "build": "vsce package",
    "install-extension": "npm run build && cursor --install-extension trace-extractor-1.0.0.vsix"
  },
  "keywords": [
    "cursor",
    "extractor",
    "data-extraction",
    "conversations",
    "markdown",
    "vscode-extension",
    "cli",
    "sqlite",
    "ai-chat"
  ],
  "author": "Mert Basaran",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mudiko/trace-extractor.git"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.0",
    "sqlite3": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "vscode": "^1.74.0"
  },
  "bin": {
    "trace-extractor": "./cli.js"
  },
  "activationEvents": [
    "onCommand:trace-extractor.selectChat",
    "onCommand:trace-extractor.selectChatJSON"
  ],
  "contributes": {
    "commands": [
      {
        "command": "trace-extractor.selectChat",
        "title": "Export Conversation to Markdown",
        "category": "Trace Extractor"
      },
      {
        "command": "trace-extractor.selectChatJSON",
        "title": "Export Conversation to JSON",
        "category": "Trace Extractor"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "trace-extractor.selectChat",
          "when": "true"
        },
        {
          "command": "trace-extractor.selectChatJSON",
          "when": "true"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "vsce": "^2.15.0"
  }
}
